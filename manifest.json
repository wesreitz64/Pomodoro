{
  "name": "Starter Sidepanel Template-filtered",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Basic template for creating Google chrome extensions with a side panel!!. Its a great day",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["sidePanel"],
  "side_panel": {
    "default_path": "sidepanel/index.html"
  },
  "action": {
    "default_icon": {
      "16": "images/tomato16.png",
      "32": "images/tomato32.png",
      "48": "images/tomato48.png",
      "128": "images/tomato128.png"
    },
    "default_title": "Open Chat Interface"
  }



}
